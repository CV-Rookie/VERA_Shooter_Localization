		if(textNum == null || textNum == '')
		{
			alert('shit!');
			return;
		}
		textListgetSiteTextList(2,textNum,7);
	});
function textListgetSiteTextList(catalogId,startId,getNum)
{
		//alert('hi');
	var data = {};
	data.catalogId = catalogId;
	data.startId = startId;
	data.getNum = getNum;
	//变查看更多为'loading'
	$('#textList > div.textListDiv > div.viewMore').replaceWith("<div class='wrapLoading'><div class='loading'></div></div>");
	$.post("/dasai/index.php/siteWidget/textList",data,function(result){
		//alert(result);
		$('#textList > div.textListDiv > div.wrapLoading').remove();
		$.each(result,function(index,item){
							if(index % 2 == 0)
				{
			var tempBlock = $("<div class='sBlock even'><a class='sText' href='/dasai/index.php/site/viewText?id="+item.textId+"&checkId="+item.checkId+"'>"+item.textTitle+"</a></div>");
				}
				else
				{
				var tempBlock = $("<div class='sBlock odd'><a class='sText' href='/dasai/index.php/site/viewText?id="+item.textId+"&checkId="+item.checkId+"'>"+item.textTitle+"</a></div>");
				}
			tempBlock.appendTo("#textList > div.textListDiv");
					});
			},'json');
}
</script>
